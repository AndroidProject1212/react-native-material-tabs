{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "member-access": false,
    "interface-name": [
      "never-prefix"
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "trailing-comma": [
      true,
      {
        "singleline": "never",
        "multiline": "never"
      }
    ],
    "jsx-no-multiline-js": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "import-spacing": true
  }
}
